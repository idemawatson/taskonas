{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["esnext", "esnext.asynciterable", "dom"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@/*": ["src/*"]
    },
    "strictNullChecks": false,
    "types": ["@types/node", "@nuxt/types", "vuetify"],
    "typeRoots": ["types"]
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
